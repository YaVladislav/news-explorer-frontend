{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "ignorePatterns": [".gitignore"],
    "rules": {
      "no-underscore-dangle": "off",
      "no-console": "off",
      "consistent-return": "off",
      "func-names": "off"
    }
}